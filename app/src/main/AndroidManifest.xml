        <activity android:name=".Activities.ClientListSelector.ClientListSelectorActivity">
        </activity>
        <activity android:name=".Activities.CierreCaja.CierreCajaActivity">
        </activity>
        <activity android:name=".Activities.NotificationsDetail.BirthdayDetail">
        </activity>
        <activity android:name=".Activities.NotificationsDetail.CadenciaDetail">
        </activity>
        <activity android:name=".Activities.Stadisticas.StadisticasActivity">
        </activity>
        <activity android:name=".Activities.Settings.SettingsActivity">
        </activity>
        <activity android:name=".Activities.Empleados.EmpleadosActivity">
        </activity>
        <activity android:name=".Activities.EmpleadoDetail.EmpleadoDetailActivity">
        </activity>
        <activity android:name=".Activities.TipoServicios.TipoServiciosActivity">
        </activity>
        <activity android:name=".Activities.NuevoTipoServicio.NuevoTipoServicioActivity">
        </activity>
    </application>

</manifest>